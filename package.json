{
  "name": "intellimen-website",
  "version": "1.0.0",
  "description": "IntelliMen - 53 Desafios Para Homens Inteligentes - Website Oficial",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "npm install && node setup.js",
    "build": "echo 'Building static assets...' && npm run setup",
    "test": "echo 'Running tests...' && node test.js",
    "deploy": "echo 'Deploying to production...'",
    "reset-db": "rm -f intellimen.db && node server.js",
    "backup-db": "cp intellimen.db intellimen_backup_$(date +%Y%m%d_%H%M%S).db"
  },
  "keywords": [
    "intellimen",
    "desafios",
    "homens",
    "desenvolvimento",
    "autoajuda",
    "masculinidade",
    "crescimento-pessoal",
    "webapp",
    "nodejs",
    "sqlite"
  ],
  "author": {
    "name": "IntelliMen Team",
    "email": "contato@intellimen.com",
    "url": "https://intellimen.com"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
